-- code qui prend une valeur dans le terminal et stock les r√©ponse dans une variable


a = {}
b = {}
x = 0

print("entrez une valeur : ")
n = io.read()
n = tonumber(n)


for i = 1, n do
    x = i/3
    if math.floor(x) == x then
        a[#a + 1] = i
    else
    b[#b + 1] = i
    end
end


function decouverte()
    for i, v in ipairs(b) do
    print(v)
    end
end

decouverte()